{
  "name": "nuxt2-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "mock": "nodemon mock/app.js --watch mock/",
    "mock-dev": "nodemon mock/app.js --watch mock/ | nuxt",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install",
    "lint": "npm run lint:js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "cheerio": "^1.0.0-rc.12",
    "cookie-universal-nuxt": "^2.2.2",
    "core-js": "^3.15.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "sass": "^1.56.1",
    "sass-loader": "^10.1.1",
    "webpack": "^4.46.0",
    "chalk": "^4.1.1",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "koa": "^2.13.4",
    "nodemon": "^2.0.20"
  }
}
